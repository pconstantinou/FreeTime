{
  "manifest_version": 2,

  "name": "Free Time",
  "description": "Quickly get a few of the free blocks of time on your calendar",
  "version": "1.0",

  "options_page": "views/options.html",
  "options_ui": {
    "page": "views/options.html",
    "chrome_style": true
  },

  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },



  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "views/options.html",
    "default_title": "See Free Time"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}